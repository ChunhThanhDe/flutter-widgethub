{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Web dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "args": [
        "--target",
        "lib/main_dev.dart",
        "--dart-define-from-file=../envs/fwh/dev.env.json",
        "-d",
        "chrome",
        "--web-port",
        "8000",
        "web-renderer",
        "html",
        "--web-browser-flag",
        "--disable-web-security"
      ]
    },
    {
      "name": "Launch dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "args": [
        "--flavor",
        "dev",
        "--target",
        "lib/main_dev.dart",
        "--dart-define-from-file=../envs/fwh/dev.env.json"
      ]
    },

    {
      "name": "Launch prod",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_prod.dart",
      "args": [
        "--flavor",
        "prod",
        "--target",
        "lib/main_prod.dart",
        "--dart-define-from-file=../envs/fwh/env.json"
      ]
    }
  ]
}
